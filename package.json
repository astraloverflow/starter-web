  "version": "2020.11.30",
  "name": "@astraloverflow/starter-web",
  "description": "A starter template for web projects",
  "repository": "",
  "scripts": {
    "test": "npm run test:codestyle && npm run test:codequality",
    "test:codestyle": "stylelint \"src/**/*.{css}\" && eslint \"src/**/*.{js,ts}\"",
    "test:codequality": "tsc --noEmit && jest",
    "format": "prettier --write",
    "dev": "parcel src/index.html",
    "prod": "parcel build src/index.html"
  },
  "browserslist": [
    "last 2 major versions and cover 95% and not dead and not ie > 1 and not op_mini all"
  ],
  "dependencies": {
    "preact": "^10.5.7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
